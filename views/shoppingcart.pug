extends layout

block content 
  h1 Your Cart
  h2='Order ID: '+ shoppingCart.orderId
  h2='Customer Name: ' + shoppingCart.customerName
  hr
  p='ID: ' + product.productID
  p='Name: ' + product.productName
  p='Brand: ' + product.productBrand
  p='Description: ' + product.productDescription
  p='Price: $' + product.productPrice
  hr
  h2='Total Items: ' + shoppingCart.itemQuantity
  h2='Amount Due: ' + shoppingCart.orderTotal